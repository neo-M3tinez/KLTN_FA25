# Kibana Alert Collector Configuration
# Copy to config.env and fill in your values

# Kibana Connection
KIBANA_URL=http://localhost:5601
KIBANA_SPACE=default

# Authentication (choose one method)

# Method 1: Username/Password
KIBANA_USERNAME=elastic
KIBANA_PASSWORD=your_password_here

# Method 2: API Key (recommended for production)
# KIBANA_API_KEY=your_api_key_here

# Collector Settings
POLL_INTERVAL=60
PROCESS_WITH_AGENT=true

# Google Gemini API Key (optional, for AI-powered analysis)
GOOGLE_API_KEY=your_gemini_api_key_here
